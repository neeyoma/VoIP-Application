          <!-- Contacts -->
          <table class="table is-indent" data-plugin="animateList" data-animate="fade" data-child="tr"
            data-selectable="selectable">
            <thead>
              <tr>
                <th class="pre-cell"></th>
                <th class="cell-30" scope="col">
                  <span class="checkbox-custom checkbox-primary checkbox-lg contacts-select-all">
                    <input type="checkbox" class="contacts-checkbox selectable-all" id="select_all"
                    />
                    <label for="select_all"></label>
                  </span>
                </th>
                <th class="cell-300" scope="col">Name</th>
                <th class="cell-300" scope="col">Phone</th>
                <th scope="col">Email</th>
                <th class="suf-cell"></th>
              </tr>
            </thead>
            <tbody>
              <% my_contacts.forEach (function(eachContact){%>
                
                <tr data-url="/slide/<%=eachContact.id%>/" data-toggle="slidePanel">
                    <td class="pre-cell"></td>
                    <td class="cell-30">
                      <span class="checkbox-custom checkbox-primary checkbox-lg">
                        <input type="checkbox" class="contacts-checkbox selectable-item" id="contacts_1"
                        />
                        <label for="contacts_1"></label>
                      </span>
                    </td>
                    <td class="cell-300">
                      <a class="avatar" href="javascript:void(0)">
                        <img class="img-fluid" src="/global/portraits/<%=Math.floor((Math.random() * 19) + 1)%>.jpg" alt="...">
                      </a>
                      <%= eachContact.c_name%>
                    </td>
                    <td class="cell-300"><%= eachContact.c_phone%></td>
                    <td><%= eachContact.c_email%></td>
                    <td class="suf-cell"></td>
                  </tr>
              <%})%>
            </tbody>
          </table>

          <ul data-plugin="paginator" data-total="50" data-skin="pagination-gap"></ul>
        </div>
